public class KnightBoard{
    public int[][] board;
    public int size;
    public KnightBoard (int s){
	board = new int[s+4][s+4];
	size = s;
    }
    public String toString(){
	String otpt = "";
	for (int x = 2, x<size+2; x++){
	    for (int y = 2, y<size+2; y++){
		otpt += board[x][y];
		if (board[x][y]>9){
		    otpt+= " ";
		}

    }
}